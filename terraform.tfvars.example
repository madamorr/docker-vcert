# Example Terraform variables file
# Copy this file to terraform.tfvars and update the values

# AWS region where resources will be created
aws_region = "us-east-1"

# Name of your existing IAM role for CodeBuild
# This role should have permissions for:
# - ECR operations (push, pull)
# - CloudWatch Logs
# - VPC access if needed
existing_codebuild_role_name = "your-existing-codebuild-role-name"

# Environment (dev, staging, prod)
environment = "dev"

# Project name (optional - defaults to vcert-lambda)
project_name = "vcert-lambda"

# Docker image tag (optional - defaults to 5.11.1)
image_tag = "5.11.1"

# GitHub repository URL for CodeBuild source
github_repo_url = "https://github.com/your-username/your-repo-name.git"